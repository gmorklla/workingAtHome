.item {
  width: auto;
  height: auto;
  overflow: hidden;
  color: white;
  font-size: 10px;
  position: absolute;
  padding: 1px;
  background-color: transparent;
  border-radius: 5px;
}

iframe {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}

.handle {
  border: 1px solid gray;
  background: rgba(20, 73, 220, 0);
  position: absolute;
  top: 0;
  left: 0;
}
